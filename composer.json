{
  "name": "arraypress/wp-register-rest",
  "version": "1.0.0",
  "description": "A streamlined WordPress REST API endpoint registration system with schema validation and route management",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-register-rest/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\Register\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-rest/issues",
    "source": "https://github.com/arraypress/wp-register-rest"
  },
  "keywords": [
    "wordpress",
    "rest-api",
    "api",
    "endpoints",
    "wp-api",
    "wp-rest",
    "rest-endpoints",
    "api-registration",
    "wp-rest-api",
    "rest-routes",
    "api-management"
  ]
}